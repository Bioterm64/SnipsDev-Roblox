local module = {}

--[[]
1 - Error
2 - Warn
3 - Info
4 - Verbose (aka. debug)
]]

module.verbosity = 4
module.header = "[SnipsDev]%s: "
module.footer = ""

function module:Error(message: string)
	if module.verbosity < 1 then return end
	warn(string.format(module.header, " Error") .. message .. module.footer)
end

function module:Warn(message: string)
	if module.verbosity < 2 then return end
	warn(string.format(module.header, " Warning") .. message .. module.footer)
end

function module:Info(message: string)
	if module.verbosity < 3 then return end
	print(string.format(module.header, "[Info]") .. message .. module.footer)
end

function module:Verbose(message: string)
	if module.verbosity < 4 then return end
	print(string.format(module.header, "[Debug]") .. message .. module.footer)
end

-- It is not affected because this is where 
-- the user really needs to know about the
-- plugin's status like:
-- > if it is enabled or not
function module:Status(message)
	print(string.format(module.header, "") .. message .. module.footer)
end

return module]]></ProtectedString>
