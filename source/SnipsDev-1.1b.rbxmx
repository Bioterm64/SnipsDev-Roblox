local Base = script.Base
--local Base = script.Parent.Base

local Dialog = require(script.Parent.ConfirmDialog)
local logger = require(script.Parent.Logger)

local module = {}
local widget = nil
local hide_widget = false

local function showColor(item, color)
	if color == nil then
		item.BackgroundTransparency = 1
		return
	end
	
	item.BackgroundTransparency = 0
	item.BackgroundColor3 = color
end

local function bindButtonControls(item)
	local hover = 
		item:GetAttribute("button_hover") 
		or Color3.fromRGB(66, 66, 66)

	local pressed = 
		item:GetAttribute("button_pressed") 
		or Color3.fromRGB(0, 170, 255)

	item.AutoButtonColor = false

	item:GetPropertyChangedSignal("GuiState"):Connect(function()
		local state = item.GuiState
		if state == Enum.GuiState.Hover then
			showColor(item, hover)
		elseif state == Enum.GuiState.Press then
			showColor(item, pressed)
		else
			showColor(item)
		end
	end)
end

function module:GetInit()
	return module.init
end

function module:Init(plugin, callbacks)
	module.plugin = plugin
	
	for _, item in Base:GetDescendants() do
		if item:IsA("GuiButton") then
			bindButtonControls(item)
		elseif item.Name == "0_reset_snippets" then
			local button: TextButton = item:FindFirstChild("Button")
			if not button then
				logger:Verbose("Failed to bind `reset snippet` button in the menu.")
				return
			end
			
			button.MouseButton1Click:Connect(callbacks.snippets)
		elseif item.Name == "zDelete" then
			local button: TextButton = item:FindFirstChild("Button")
			if not button then
				logger:Verbose("Failed to bind `clear history` button in the menu.")
				return
			end
			
			button.MouseButton1Click:Connect(callbacks.history)
		end
	end
	
	module.init = true
	
	local widgetInfo = DockWidgetPluginGuiInfo.new(
		Enum.InitialDockState.Float, false, true, 600, 300, 200, 100
	)

	widget = module.plugin:CreateDockWidgetPluginGui("OptionsMenuWidget", widgetInfo)
	widget.Title = "SnipsDev Menu"
	Base.Parent = widget
end

local function setRawText(content, name)
	return function()
		module.plugin:SetSetting("raw code", content)
		logger:Status("Reverted back to'"..name.."'!")
	end
end

function module:Show(config)
	widget.Enabled = not widget.Enabled
	
	local history = module.plugin:GetSetting("_history") or {}
	local history_format = config.timestamp_format or "YYYY-MM-DD HH:mm:ss.SSS"
	local history_locale = config.timestamp_locale or "en-us"
	
	for _, item in pairs(Base.Content['1_history']:GetChildren()) do
		if not item:IsA("Frame") then continue end
		item:Destroy()
	end
	
	for i, data in pairs(history) do
		local content = data['content']
		
		local timestamp = DateTime.fromUnixTimestampMillis(data['time'])
		local name = timestamp:FormatLocalTime(history_format, history_locale)
		
		local newItem = script.item:Clone()
		newItem.Name = #history - i
		newItem.Parent = Base.Content['1_history']
		
		newItem.Button.Text = name
		bindButtonControls(newItem.Button)
		
		newItem.Button.MouseButton1Click:Connect(function()
			if not Dialog:GetInit() then
				Dialog:Init(module.plugin)
			end
			
			logger:Status("Preview of the content:\n"..content)
			
			-- FOR SOME REASON, THIS IS CAUSING PROBLEMS
			local message = "Are you sure to use '"..name.."'?\nYou cannot undo this operation! (Preview is in the console)"
			local title = "Reverting back to '"..name.."'"
			
			Dialog:Show(
				message,
				title,
				setRawText(content, name)
			)
		end)
	end
end

return module]]></ProtectedString>
