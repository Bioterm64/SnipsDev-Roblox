local module = {}

local Packages = script.Parent:WaitForChild("Modules")
local Dialog = require(Packages:WaitForChild("ConfirmDialog"))
local OptionsMenu = require(Packages:WaitForChild("OptionsDialog"))
local AutoComplete = require(Packages:WaitForChild("AutoComplete"))
local SnippetConfigureHandler = require(Packages:WaitForChild("SnippetConfigureHandler"))
local Editor = require(Packages:WaitForChild("Editor"))
local logger = require(Packages:WaitForChild("Logger"))

local ScriptEditorService = game:GetService("ScriptEditorService")

local function resetSnippets()
	if SnippetConfigureHandler.editor_script_proxy then
		SnippetConfigureHandler.editor_script_proxy:CloseAsync()
		task.delay(0.1, Editor.Close)
	end

	module.plugin:SetSetting("raw code", nil)
	AutoComplete:ReloadSnippets()
	logger:Status("Snippets have been reset.")
end

local function clearHistory()
	local history = module.plugin:GetSetting("_history") or {}
	if #history > 0 then
		module.plugin:SetSetting("_history", {})
		logger:Status("Cleared the history.")
	end
end


local function openPrompt(message, title, confirm_callback, cancel_callback)
	if not Dialog:GetInit() then
		Dialog:Init(module.plugin, false)
	end

	Dialog:Show(
		message,
		title,
		confirm_callback,
		cancel_callback
	)
end

function promptResetSnippets()
	openPrompt(
		"Are you sure to reset your snippets?\nYou cannot undo this operation!",
		"Confirm Reset",
		resetSnippets
	)
end

function promptClearHistory()
	openPrompt(
		"Are you sure to clear the history of your snippets?\nYou cannot undo this operation!",
		"Confirm Clear History",
		clearHistory
	)
end

function module:Initialize(plugin)
	module.plugin = plugin

	AutoComplete:Unregister()
	if module.plugin:GetSetting("enabled") then
		AutoComplete:Register()
	end
end

function module.onMenu()
	if not OptionsMenu:GetInit() then
		OptionsMenu:Init(module.plugin, {
			snippets = promptResetSnippets,
			history = promptClearHistory,
		})
	end
	
	OptionsMenu:Show(AutoComplete:GetConfiguration())
end

function module.onEnabled()
	local enabled = not (module.plugin:GetSetting("enabled") or false)
	module.plugin:SetSetting("enabled", enabled)

	if enabled then
		AutoComplete:Register()
	else
		AutoComplete:Unregister()
	end

	logger:Status("Snippets typehinting mode: "..tostring(enabled))
end


return module
]]></ProtectedString>
