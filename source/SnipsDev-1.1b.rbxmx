local ScriptEditorService = game:GetService("ScriptEditorService")

local SnippetParser = require(script.Parent:WaitForChild("SnippetParser"))
local logger = require(script.Parent:WaitForChild("Logger"))
local constants = require(script.Parent.Constants)
local _types = require(script.Types)

local module = {
	snippets = {}
}

local setting_name = "_#settings"
local configuration = {}

function module:Initialize(plugin)
	module.plugin = plugin
end

local function checkIfRunningOk()
	return module.plugin:GetSetting("enabled") and type(configuration.reload_snippets) == "number"
end

local timer_running = false
local function doAutoReloadInterval(interval)
	if timer_running then
		logger:Verbose("Another AutoReload timer already exists, skipping...")
		return 
	end
	
	logger:Verbose("Started AutoReload timer.")
	
	timer_running = true
	local last_save = time()
	while checkIfRunningOk() do
		wait(interval)

		if not checkIfRunningOk() then continue end

		module:ReloadSnippets()
	end
	
	timer_running = false
	logger:Verbose("Stopped AutoReload timer.")
end

function module:IgnoreEditor(editor)
	module.ignoreEditor = editor
end

local function isAnEnding(char)
	-- A space, tab or nothing
	
	return char == " " or char == "	" or char == ""
end

local function getStartOfString(line: string, start_pos)
	start_pos = start_pos - 1

	while start_pos >= 1 do
		if isAnEnding(line:sub(start_pos, start_pos)) then return start_pos end
		start_pos += -1
	end

	return 0
end

local function isSamePrefix(prefix: string, line: string)
	-- details.prefix:sub(1, 1) ~= line:sub(start, start)
	
	if configuration['case_sensitive'] ~= true then
		prefix = prefix:lower()
		line = line:lower()
	end
	
	for i = 1, #prefix do
		-- stop if its a normal ending
		if isAnEnding(line:sub(i, i)) then return true end
		
		if prefix:sub(i, i) ~= line:sub(i, i) then return false end
	end
	
	return true
end

function module.autoCompleteCallback(request: _types.Request, response: _types.Response): Response
	local cursorPosition = request.position
	local textDocument = request.textDocument

	if textDocument.document == module.ignoreEditor then return response end
	
	if configuration.reload_snippets == "always" then
		module:ReloadSnippets()
	end
	
	local line: string = textDocument.document:GetLine(cursorPosition.line)
	local start = getStartOfString(line, cursorPosition.character) + 1
	
	local always_on_top
	if configuration.always_on_top ~= nil then
		always_on_top = configuration.always_on_top
	else 
		always_on_top = true
	end

	for snippet_label, details: _types.SnippetData in pairs(module.snippets) do
		-- details: item details of the snippet
		
		if not isSamePrefix(details.prefix, line:sub(start, #line)) then continue end
		
		local snippet_kind = details.kind	-- assuming that it is already an Enum
		if type(snippet_kind) == "string" then 
			snippet_kind = Enum.CompletionItemKind[snippet_kind] 
		end
		if not snippet_kind then
			snippet_kind = Enum.CompletionItemKind.Snippet
		end

		local item: _types.ResponseItem = {
			label = details.prefix,
			kind = snippet_kind,
			preselect = always_on_top,
			textEdit = {
				newText = details.body,
				replace = {
					start = {
						line = cursorPosition.line,
						character = start
					}, 
					['end'] = {
						line = cursorPosition.line,
						character = cursorPosition.character,
					}
				},
			},
			documentation = {
				value = details.description or ""
			},
			detail = snippet_label or nil,
			codeSample = details.example or details.body,
			learnMoreLink = details.link,
		}

		table.insert(response.items, item)
	end

	return response
end

local function getSnippetFromStorage()
	local currentSnippets = SnippetParser:GetSnippetData(module.plugin:GetSetting("raw code")) or {}
	configuration = currentSnippets[setting_name] or {}
	
	for name, snippet in pairs(currentSnippets) do
		--currentSnippets[name] = nil
		
		if name == setting_name then
			currentSnippets[name] = nil
			continue
		end
		
		if snippet['prefix'] == nil or snippet['body'] == nil then
			logger:Verbose("Invalid snippet data for "..name)
			currentSnippets[name] = nil
		end
	end
	
	return currentSnippets
end

function module:GetConfiguration()
	return configuration
end

function module:ReloadSnippets()
	if not module.plugin:GetSetting("enabled") then return end
	module.snippets = getSnippetFromStorage() 
	
	logger.verbosity = configuration.logging_level or 2
	
	logger:Verbose("Snippets reloaded")
end

function module:Register()
	ScriptEditorService:RegisterAutocompleteCallback(
		constants.registry_name, 1, module.autoCompleteCallback
	)
	module:ReloadSnippets()
	
	if type(configuration.reload_snippets) == "number" then
		task.spawn(doAutoReloadInterval, configuration.reload_snippets)
	end
end

function module:Unregister()
	pcall(function()
		ScriptEditorService:DeregisterAutocompleteCallback(constants.registry_name)
	end)
end

return module
