local ScriptEditorService = game:GetService("ScriptEditorService")

local Packages = script.Parent
local Editor = require(Packages:WaitForChild("Editor"))
local AutoComplete = require(Packages:WaitForChild("AutoComplete"))
local constants = require(Packages:WaitForChild("Constants"))
local logger = require(Packages:WaitForChild("Logger"))

local module = {}

function module:Initialize(plugin)
	module.plugin = plugin
end

local listeners = {}
local isOpen = false
local text = ""
module.editor_script = nil
module.editor_script_proxy = nil

local function saveCode(text)
	module.plugin:SetSetting("raw code", text)
	
	local max_history = AutoComplete:GetConfiguration().backup_history or 10
	if max_history == 0 then return end
	
	local history = module.plugin:GetSetting("_history") 
	
	if not history then
		logger:Verbose("[Warning] History returned none. This is a lost of history.")
		history = {}
	end
		
	if max_history > 0 and #history >= max_history then
		-- Remove last entry (which is at the front)
		table.remove(history, 1)
		logger:Verbose("History is overflowing. Removed previous items.")
	end
	
	table.insert(history, {
		['time'] = DateTime.now().UnixTimestampMillis,
		content = text,
	})
	
	module.plugin:SetSetting("_history", history)
end

local function doAutoSaveInterval(interval)
	logger:Verbose("Started autosave timer.")

	local last_save = time()
	while isOpen do
		wait(interval)

		if not isOpen then continue end

		saveCode(text)
		logger:Info("Snippets Saved!")
	end

	logger:Verbose("Stopped autosave timer.")
end

local function removeListeners()
	for _, item in pairs(listeners) do item:Disconnect() end
	table.clear(listeners)
	isOpen = false
end

local function addListeners()
	if #listeners > 0 then return end

	isOpen = true

	local auto_save = AutoComplete:GetConfiguration().auto_save

	if type(auto_save) == "number" then
		logger:Verbose("`auto_save` type was a number.")
		task.spawn(doAutoSaveInterval, auto_save)
	end

	listeners[1] = ScriptEditorService.TextDocumentDidChange
		:Connect(function(currentEditor)
			if currentEditor ~= module.editor_script_proxy then return end
			text = currentEditor:GetText()

			if auto_save == "always" then
				saveCode(text)

				-- Cannot be info because it would be too annoying
				logger:Verbose("Snippets Saved!")
			end
		end)

	listeners[2] = ScriptEditorService.TextDocumentDidClose
		:Connect(function(currentEditor)
			if currentEditor ~= module.editor_script_proxy then return end

			removeListeners()
			module.editor_script = nil
			module.editor_script_proxy = nil
			AutoComplete:IgnoreEditor(nil)

			Editor:Close()

			-- simple loading fail test case
			if text == "" then
				logger:Error("Failed to save snippets. Did you keep reloading this plugin?")
				return
			end

			-- Should never placed inside a condition.
			saveCode(text)
			AutoComplete:ReloadSnippets()

			logger:Info("Snippets saved!")
		end)
end

function module.ShowConfigureSnippets()
	module.editor_script = Editor:CreateBlankEditor("Edit Snippets")

	local code = module.plugin:GetSetting("raw code")
	module.editor_script.Source = code or constants.header

	addListeners()
	module.plugin:OpenScript(module.editor_script)

	task.defer(function()
		local document = ScriptEditorService:FindScriptDocument(module.editor_script)
		while not document do
			wait()
			document = ScriptEditorService:FindScriptDocument(module.editor_script)
		end

		text = code
		module.editor_script_proxy = document
		AutoComplete:IgnoreEditor(document)
	end)
end

return module
