local module = {}

local widget = nil
local DialogFrame = nil

local function themeSync()
	local connections = {}
	
	local function handler()
		for _, conn in pairs(connections) do conn:Disconnect() end
		
		local currentTheme = settings().Studio.Theme
		
		local dark_color: Color3 = currentTheme:getColor(Enum.StudioStyleGuideColor.Dark)
		local highlight = Color3.new(1 - dark_color.R, 1 - dark_color.G, 1 - dark_color.B)
		
		DialogFrame.Message.TextColor3 = currentTheme:getColor(Enum.StudioStyleGuideColor.MainText)
		DialogFrame.BackgroundColor3 = currentTheme:getColor(Enum.StudioStyleGuideColor.MainBackground)
		
		-- No (MainButtonColor)
		DialogFrame.No.TextColor3 = currentTheme:getColor(Enum.StudioStyleGuideColor.DialogMainButtonText)
		table.insert(connections, DialogFrame.No.MouseEnter:Connect(function()
			local color: Color3 = currentTheme:getColor(Enum.StudioStyleGuideColor.DialogMainButton)
			DialogFrame.No.BackgroundColor3 = color:Lerp(highlight, 0.2)
		end))

		table.insert(connections, DialogFrame.No.MouseLeave:Connect(function()
			DialogFrame.No.BackgroundColor3 = currentTheme:getColor(Enum.StudioStyleGuideColor.DialogMainButton)
		end))
		
		-- Yes (DefaultButtonColor)
		DialogFrame.Yes.TextColor3 = currentTheme:getColor(Enum.StudioStyleGuideColor.DialogButtonText)
		table.insert(connections, DialogFrame.Yes.MouseEnter:Connect(function()
			local color: Color3 = currentTheme:getColor(Enum.StudioStyleGuideColor.DialogButton)
			DialogFrame.Yes.BackgroundColor3 = color:Lerp(highlight, 0.2)
		end))

		table.insert(connections, DialogFrame.Yes.MouseLeave:Connect(function()
			DialogFrame.Yes.BackgroundColor3 = currentTheme:getColor(Enum.StudioStyleGuideColor.DialogButton)
		end))
	end
	
	handler()
	
	settings().Studio.ThemeChanged:Connect(handler)
end

function module:Init(plugin, hide_widget)
	local widgetInfo = DockWidgetPluginGuiInfo.new(
		Enum.InitialDockState.Float, true, true, 400, 150, 250, 200
	)

	widget = plugin:CreateDockWidgetPluginGui("ConfirmDialog", widgetInfo)
	widget.Enabled = not (hide_widget or true)
	widget.Title = "Prompt"
	
	DialogFrame = script.Parent["DialogFrame"]
	DialogFrame.Parent = widget
	
	themeSync()
end

local event_confirm = nil
local event_cancel = nil

local function resetBindableEvents()
	if event_confirm then
		event_confirm:Disconnect()
	end

	if event_cancel then
		event_cancel:Disconnect()
	end
end

function module:Show(message, title, confirm_callback, cancel_callback)
	DialogFrame.Message.Text = message
	widget.Title = title or "Confirm Operation"
	widget.Enabled = true

	resetBindableEvents()

	event_confirm = DialogFrame.Yes.MouseButton1Click:Connect(function()
		widget.Enabled = false
		
		if not confirm_callback then return end
		confirm_callback()
	end)

	event_cancel = DialogFrame.No.MouseButton1Click:Connect(function()
		widget.Enabled = false
		
		if not cancel_callback then return end
		cancel_callback()
	end)
end

return module]]></ProtectedString>
